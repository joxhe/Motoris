generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Appointment {
  id          String   @id @default(cuid())
  vehicleId   String
  customerId  String
  date        DateTime
  status      String   @default("pending")
  description String?
  createdAt   DateTime @default(now())
}
